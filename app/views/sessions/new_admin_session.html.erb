
  <body>
  <div class = "welcome-container">
      <div class = "welcome">
        <h1>Co-Finder</h1>
        <h3>The best way to find co-founders and build teams</h3>
      </div>


  </div>


<div class= "login-container" >   
  <div class= "login-form" >   
  <%= form_tag admin_session_path do %>


    <h2>Login as an Admin! </h2>
    
    
    <p><%= label :admin, :email %></p>
    <div>
    <p><%= text_field :admin, :email %> </p>
    </div>

    <p><%= label :admin, :password %></p>
    <div >
    <p><%= password_field :admin, :password %></p>
    </div>


    <%= submit_tag "Log In" %>
    <%end%>  
    <p><%= link_to 'Sign Up as admin', new_admin_path %></p>
  </div>

</div>


</body>

